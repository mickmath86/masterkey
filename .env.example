# Environment Variables Template
# Copy this file to .env.local and fill in your actual values

# RapidAPI Key for Zillow APIs (server-side only)
# Get your key from: https://rapidapi.com/apimaker/api/zillow-com1
# Used for both property lookup and rental estimates
RAPIDAPI_KEY=your_rapidapi_key_here

# Form webhook URL for questionnaire submissions
NEXT_PUBLIC_FORM_WEBHOOK_URL=https://your-webhook-url.com/webhook

# Google Maps API Key (if needed)
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Sanity CMS Configuration (if using)
NEXT_PUBLIC_SANITY_PROJECT_ID=your_sanity_project_id
NEXT_PUBLIC_SANITY_DATASET=production
SANITY_API_TOKEN=your_sanity_api_token
